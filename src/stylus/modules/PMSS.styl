.all 
    margin-left 300px
    white-space nowrap
    font-size 14px
.pms-bt
    background-color #00AE9B
    color white
    padding 10px
    margin-right 50px
    position relative
    .PMS-img,
    .PMS-imgs
        position absolute
        right -30px
        top 5px
        color #A6A6A6
    .PMS-imgs
        display none
.pms-xing
    color red
    margin-left 10px
    font-size 18px
.pms-addwarehouse
    margin-right 50px
    border 1px solid #050505
    padding 6px 12px
    display none
    .pms-add
        border 1px solid #F89D30
        padding 10px
    .pms-addheader
        background-color #F9A94A
        color #FFFFFF
        padding 6px 12px
        margin -10px -10px 15px -10px
        .fa-plus-square,
        .fa-play,
        .fa-pause,
        .fa-repeat
            margin-left 3%
            cursor pointer
        .fa-comments
            vertical-align super
.pms-business
    background-color #F1F1F1
.form-control.pms-input,
.form-control.pms-select
    box-shadow none
    border 1px solid gray
.pms-set
    width 18%
    white-space normal
.pms-distribution
    border 1px solid #808080
    width 12%
    height 34px
    padding 6px 12px
    border-radius 4px
    background-color white
.add-btn
    margin-top 7px